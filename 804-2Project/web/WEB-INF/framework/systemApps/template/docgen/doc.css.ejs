body {
    margin: 5px;
    color: #222222;
    background-color: #eeeefc;
    // background-color: #ffffff;
    font-size: 1.1em;
}

a {
    color:#204a87;
    font-weight:bold;
    text-decoration:none;
}

a:hover {
    color:#3465a4;
    text-decoration:underline;
}

pre {
    // font-family: serif;
}

.title {
    text-transform: uppercase;
    font-weight: bold;
}

.annotation {
    color: #aa0000;
    font-size: 70%;
}

.mainPage .normal {
    margin-top: 20px;
}

.mainPage .footer {
    position: fixed;
    bottom: 0px;
    width: 100%;
    margin-top: 40px;
    padding-top: 10px;
    border-top: 1px solid #000000;
}

.moduleList .title {
    padding-bottom: 10px;
}

.moduleList a {
    padding-left: 10px;
    text-transform: none;
}

.moduleList .projectReference {
    padding-left: 0px;
    text-transform: uppercase;
}

.moduleList .name {
    padding-bottom: 2px;
}

.moduleOverview .name {
    text-transform: uppercase;
    font-weight: bold;
    margin-bottom: 20px;
}

.moduleOverview a {
    padding-left: 10px;
    text-transform: none;
}

.moduleOverview .projectReference {
    padding-left: 0px;
    text-transform: uppercase;
}

.moduleOverview .memberName {
    margin-left: 10px;
}

.moduleOverview .classMemberName {
    margin-left: 40px;
}

.moduleContents .name {
    font-size: 150%;
    text-transform: uppercase;
    margin-bottom: 20px;
    border-bottom: 2px solid #202a67;
}

.moduleContents .comment {
    margin-left: 10px;
    padding: 5px;
    // border-bottom: 2px solid #000000;
    // background-color: #ffffff;
}

.moduleContents .members {
    margin-left: 10px;
}

.moduleContents .members .member {
    // border-top: 2px solid #000000;
    padding-bottom: 20px;
}

.moduleContents .members .memberType_class {
    // border-bottom: 4px solid #202a67;
}

.moduleContents .members .memberType_class .member {
    // border-top: 1px solid #000000;
    // border-bottom: 0px solid #000000;
}

.moduleContents ..members .memberType_class .section {
    font-weight: bold;
    margin-top: 20px;
    margin-bottom: 10px;
    text-decoration: underline;
}

.moduleContents .members .description {
    font-size: 120%;
    margin-top: 10px;
    margin-left: 10px;
    padding: 5px;
    font-weight: bold;
    color: #222222;
    border-top: 4px solid #202a67;
    border-bottom: 1px solid #202a67;
}

.moduleContents .members .comment {
    margin-left: 10px;
    // background-color: #ffffff;
    padding: 5px;
    border-bottom: 0px solid #000000;
}

.moduleContents table {
    font-size: 100%;
}

.moduleContents .metadataHeader {
    font-weight: bold;
    margin-top: 20px;
    margin-bottom: 10px;
    text-decoration: underline;
}

.moduleContents .metadata {
    font-size: 100%;
    border-left: 2px solid #202a67;
    padding-left: 4px;
}

.moduleContents .metadata th {
    width: 10%;
    text-align: left;
}

.moduleContents .metadataComment th {
    width: 40%;
    text-align: left;
    font-size: 100%;
}

.moduleContents .metadata td {
    vertical-align: top;
    border-top: 1px dotted #202a67;
}

.moduleContents .footer {
    margin-top: 40px;
    padding-top: 10px;
    border-top: 1px solid #000000;
}
